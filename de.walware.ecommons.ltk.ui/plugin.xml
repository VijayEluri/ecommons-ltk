            name="%commands_SelectLast_name"
            description="%commands_SelectLast_description"
            />
      <command
            id="de.walware.ecommons.ltk.commands.SpecificContentAssist"
            categoryId="de.walware.statet.base.commandCategorys.StatetSource"
            name="%commands_SpecificContentAssist_name"
            description="%commands_SpecificContentAssist_description">
         <commandParameter
               id="categoryId"
               name="%commands_SpecificContentAssist_param_CategoryId_name"
               optional="false"
               values="de.walware.ecommons.ltk.internal.ui.SpecificContentAssistParameterValues"/>
