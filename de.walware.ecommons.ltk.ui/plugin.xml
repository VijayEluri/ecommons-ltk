            commandId="de.walware.ecommons.ltk.commands.InsertAssignment"
            contextId="de.walware.ecommons.text.contexts.TextEditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+="/>
