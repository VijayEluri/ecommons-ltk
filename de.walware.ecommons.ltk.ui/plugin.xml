<plugin>
   
   <extension-point id="de.walware.ecommons.ltk.advancedContentAssist"
         schema="schema/advancedContentAssist.exsd"
         name="Point to register categories and computers for advanced content assist in editors"/>
   
   <extension-point id="de.walware.ecommons.ltk.advancedInfoHover"
         schema="schema/advancedInfoHover.exsd"
         name="Point to register information hovers"/>
   
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="de.walware.ecommons.text.contexts.TextEditor"
            parentId="org.eclipse.ui.textEditorScope"
            name="%contexts_TextEditorScope_name"
            description="%contexts_TextEditorScope_description">
      </context>
      <context
            id="de.walware.ecommons.base.contexts.StructuredElementViewer"
            parentId="org.eclipse.ui.contexts.window"
            name="%contexts_StructuredElementViewer_name">
            description="%contexts_StructuredElementViewer_description">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="de.walware.ecommons.ltk.contexts.EditSourceActionSet"
            label="StatET Source Editing">
      </actionSet>
   </extension>
   
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="de.walware.statet.base.expressions.EditSourceActionSet">
              <with variable="activeContexts">
                 <iterate
                       operator="or">
                    <equals value="de.walware.ecommons.ltk.contexts.EditSourceActionSet">
                    </equals>
                 </iterate>
              </with>
      </definition>
   </extension>
   
   <extension
         point="org.eclipse.ui.commands">
      <!-- edit -->
      <command
            id="de.walware.ecommons.base.commands.CopyElementName"
            categoryId="org.eclipse.ui.category.edit"
            name="%commands_CopyElementName_name"
            description="%commands_CopyElementName_description"
            />
      <command
            id="de.walware.ecommons.base.commands.SelectExpandEnclosing"
            categoryId="org.eclipse.ui.category.edit"
            name="%commands_SelectEnclosing_name"
            description="%commands_SelectEnclosing_description"
            />
      <command
            id="de.walware.ecommons.base.commands.SelectExpandNext"
            categoryId="org.eclipse.ui.category.edit"
            name="%commands_SelectNext_name"
            description="%commands_SelectNext_description"
            />
      <command
            id="de.walware.ecommons.base.commands.SelectExpandPrevious"
            categoryId="org.eclipse.ui.category.edit"
            name="%commands_SelectPrevious_name"
            description="%commands_SelectPrevious_description"
