            commandId="de.walware.ecommons.ltk.commands.AddDocComment"
            contextId="de.walware.ecommons.text.contexts.TextEditor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+#"/>
